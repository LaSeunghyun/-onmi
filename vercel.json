{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/cron/crawl.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/cron/crawl",
      "dest": "api/cron/crawl.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/crawl",
      "schedule": "0 0 * * *"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "regions": ["icn1"]
}

